<div id="friendCardsContainer">

    <div class="fCardHeader">
        <img src = {{userImg}} alt = "{{userName}}'s profile image"/>
        <h2>{{userName}}</h2>
        

    </div>

    <div class="fCardBody">
        <p>Location: {{location}}</p>
        {{#each ratings}}
        <p>Movie Name: {{name}} </p>
        <p>OMDB Rating: {{omdbRating}}</p>
        <p>Movie Friend's rating: {{userRating}}</p>
        {{/each}}
    </div>

</div>